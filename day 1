//day one code
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x;
	cin>>t; // input of no.of test cases
	for(int i=0;i<t;i++) // loop to take input of multiple value ,loop will run t no of times that is no of test cases
	{
	   cin>>x; 
	   if(x>=2000) // condition statement
	   {
	       cout<<"YES"<<endl;
	   }
	   else if(x==2000) // condition statement
	   {
	       cout<<"YES"<<endl;
	   }
	   else // condition statement
	   {
	       cout<<"NO"<<endl;
	   }
	}
	
	return 0;
}
