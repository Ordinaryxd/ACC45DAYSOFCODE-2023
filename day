#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,i;
	cin>>t;
	while(t--)
	{
	    cin>>n;
	    if(n%2==0)
	    {
	        cout<<n/2<<n/3<<n/3<<endl;
	    }
	    else
	    {
	        cout<<n/3<<n/3<<n/2<<endl;
	    }
	}
	return 0;
}
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,m,i;
	cin>>t;
	while(t--)
	{
	    cin>>n>>m;
	    i=n/m;
	    if(n%m==0)
	    { if(i%2==0)
	            {cout<<"yes"<<endl;}
	       else
	       {cout<<"no"<<endl;}
	    }
	    else
	    {cout<<"no"<<endl;}
	}
	return 0;
}
