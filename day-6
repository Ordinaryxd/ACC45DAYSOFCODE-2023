//intermidiate
#include<iostream>
using namespace std;
int main()
{
	int t;
	cin>>t;
	while(t--)
	{
		int count=0;
		int d;
		cin>>d;
		char a[d];
		for(int i=0;i<d;i++)
		{
			cin>>a[i];
			if(a[i]=='5'||a[i]=='0')
			count++;
		}
		if(count==0)
		cout<<"No"<<endl;
		else
		cout<<"Yes"<<endl;
	}
	return 0;
}
// beginner
#include <iostream>
#include <cmath>
using namespace std;

int main() {
	int t,i;
	int n;
	cin>>t;
	for(i=0;i<t;i++)
	{
	    cin>>n;
	    if(n%4==0)
	    {
	        cout<<n/4<<endl;
	    }
	    else
	    { 
	        cout<<n/4+1<<endl;
	    }
	}
	return 0;
}
