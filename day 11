// beginners
#include <iostream>
using namespace std;

int main() {
	// your code goes here 
	int t,n,m;
	cin>>t;
	while(t--)
	{
	    cin>>n>>m;
	    if(36*m>=n)
	    {
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}

#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n,c;
	    cin>>n;
	    int a[n],b[n];
	    for(int i=0;i<n;i++)
	        cin>>a[i];
	    for(int i=0;i<n;i++)
	    {
	        int count=0;
	        for(int j=0;j<n;j++)
	        {
	            if(a[i]==a[j])
	            {
	                count++;
	            }
	        }
	        b[i]=count;
	    }
	    sort(b,b+n);
	    c=n-b[n-1];
	    cout<<c<<endl;
	}
	return 0;
}
